php_value xdebug.var_display_max_depth 2
php_value xdebug.show_local_vars On
php_value xdebug.show_mem_delta On
php_value xdebug.trace_options 4
php_value xdebug.collect_includes On
php_value xdebug.collect_params 4
php_value xdebug.collect_return On
php_value xdebug.collect_vars On
php_value xdebug.dump_globals On
php_value xdebug.scream 1

RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^ index.php [L]
